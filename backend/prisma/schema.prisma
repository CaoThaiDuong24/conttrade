model carts {
  id                    String       @id @default(dbgenerated("gen_random_uuid()"))
  user_id               String       @unique
  session_id            String?      @db.VarChar(255)
  status                CartStatus   @default(ACTIVE)
  expires_at            DateTime?
  converted_to_rfq_id   String?
  converted_to_order_id String?
  created_at            DateTime     @default(now())
  updated_at            DateTime     @default(now()) @updatedAt
  cart_items            cart_items[]
  order                 orders?      @relation(fields: [converted_to_order_id], references: [id])
  rfq                   rfqs?        @relation(fields: [converted_to_rfq_id], references: [id])
  user                  users        @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([status])
  @@index([expires_at])
}

model cart_items {
  id                     String    @id @default(dbgenerated("gen_random_uuid()"))
  cart_id                String
  listing_id             String
  quantity               Int       @default(1)
  deal_type              DealType?
  rental_duration_months Int       @default(0)
  price_snapshot         Decimal   @db.Decimal(15, 2)
  currency               String    @default("VND") @db.VarChar(3)
  notes                  String?
  added_at               DateTime  @default(now())
  updated_at             DateTime  @default(now()) @updatedAt
  selected_container_ids String[]  @default([])
  cart                   carts     @relation(fields: [cart_id], references: [id], onDelete: Cascade)
  listing                listings  @relation(fields: [listing_id], references: [id], onDelete: Cascade)

  @@unique([cart_id, listing_id, deal_type, rental_duration_months], name: "cart_item_key")
  @@index([cart_id])
  @@index([listing_id])
}

model rental_contracts {
  id                           String                       @id @default(uuid())
  listing_id                   String
  container_id                 String?
  seller_id                    String
  buyer_id                     String
  order_id                     String?
  start_date                   DateTime                     @db.Date
  end_date                     DateTime                     @db.Date
  rental_price                 Decimal                      @db.Decimal(15, 2)
  rental_currency              String                       @default("VND") @db.VarChar(3)
  rental_unit                  String                       @db.VarChar(50)
  deposit_amount               Decimal?                     @db.Decimal(15, 2)
  deposit_currency             String?                      @db.VarChar(3)
  deposit_paid                 Boolean                      @default(false)
  deposit_paid_at              DateTime?
  deposit_returned             Boolean                      @default(false)
  deposit_return_date          DateTime?                    @db.Date
  deposit_return_amount        Decimal?                     @db.Decimal(15, 2)
  status                       RentalContractStatus         @default(ACTIVE)
  payment_status               RentalPaymentStatus          @default(PENDING)
  total_amount_due             Decimal?                     @db.Decimal(15, 2)
  total_paid                   Decimal                      @default(0) @db.Decimal(15, 2)
  late_fees                    Decimal                      @default(0) @db.Decimal(15, 2)
  days_overdue                 Int                          @default(0)
  late_fee_rate                Decimal?                     @db.Decimal(10, 2)
  late_fee_unit                String?                      @db.VarChar(20)
  contract_number              String?                      @unique @db.VarChar(100)
  contract_pdf_url             String?
  terms_and_conditions         String?
  special_notes                String?
  pickup_depot_id              String?
  return_depot_id              String?
  auto_renewal                 Boolean                      @default(false)
  renewal_count                Int                          @default(0)
  last_renewed_at              DateTime?
  next_renewal_date            DateTime?                    @db.Date
  pickup_condition             String?
  pickup_photos                String[]
  pickup_inspection_date       DateTime?
  pickup_inspector_name        String?                      @db.VarChar(255)
  return_condition             String?
  return_photos                String[]
  return_inspection_date       DateTime?
  return_inspector_name        String?                      @db.VarChar(255)
  damage_report                String?
  damage_cost                  Decimal?                     @db.Decimal(15, 2)
  created_at                   DateTime                     @default(now())
  updated_at                   DateTime                     @updatedAt
  completed_at                 DateTime?
  deleted_at                   DateTime?
  quantity                     Int                          @default(1)
  auto_renewal_enabled         Boolean                      @default(false)
  auto_renewal_duration_months Int?
  previous_contract_id         String?
  renewal_contract_id          String?
  maintenance_logs             container_maintenance_logs[]
  buyer                        users                        @relation("BuyerContracts", fields: [buyer_id], references: [id], onDelete: Cascade)
  container                    listing_containers?          @relation(fields: [container_id], references: [id])
  listing                      listings                     @relation(fields: [listing_id], references: [id], onDelete: Cascade)
  order                        orders?                      @relation(fields: [order_id], references: [id])
  pickup_depot                 depots?                      @relation("PickupDepot", fields: [pickup_depot_id], references: [id])
  return_depot                 depots?                      @relation("ReturnDepot", fields: [return_depot_id], references: [id])
  seller                       users                        @relation("SellerContracts", fields: [seller_id], references: [id], onDelete: Cascade)
  payments                     rental_payments[]

  @@index([listing_id, deleted_at])
  @@index([seller_id, deleted_at])
  @@index([buyer_id, deleted_at])
  @@index([container_id, deleted_at])
  @@index([order_id])
  @@index([status, deleted_at])
  @@index([payment_status])
  @@index([end_date, deleted_at])
  @@index([start_date])
  @@index([contract_number])
}

